{
  "name": "bars",
  "version": "1.0.0",
  "description": "BARS",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw --quiet webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist-prod": "npm run remove-dist-prod && mkdir dist\\prod",
    "clean-dist-test": "npm run remove-dist-test && mkdir dist\\test",
    "remove-dist-prod": "node_modules/.bin/rimraf dist\\prod",
    "remove-dist-test": "node_modules/.bin/rimraf dist\\test",
    "build-prod:html": "babel-node tools/buildHtml.js --prod",
    "build-test:html": "babel-node tools/buildHtml.js",
    "prebuild-prod": "npm-run-all clean-dist-prod test lint build-prod:html",
    "prebuild-test": "npm-run-all clean-dist-test test lint build-test:html",
    "build-prod": "babel-node tools/build.js --prod",
    "build-test": "babel-node tools/build.js",
    "postbuild-prod": "babel-node tools/distServer.js --prod",
    "postbuild-test": "babel-node tools/distServer.js"
  },
  "author": "California Department of Technology",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "6.22.0",
    "babel-polyfill": "6.22.0",
    "jwt-decode": "2.1.0",
    "react": "15.4.2",
    "react-bootstrap": "0.30.7",
    "react-dom": "15.4.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.4",
    "react-tooltip": "^3.3.0",
    "reactable": "0.14.1",
    "redux": "3.6.0",
    "redux-form": "6.5.0",
    "redux-thunk": "2.0.1",
    "whatwg-fetch": "2.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.22.0",
    "bootstrap": "3.3.7",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "enzyme": "2.7.1",
    "eslint": "3.14.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.9.0",
    "eslint-watch": "2.1.14",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "express": "4.14.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "jsdom": "9.9.1",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "mocha": "3.2.0",
    "nock": "9.0.2",
    "npm-run-all": "4.0.1",
    "open": "0.0.5",
    "react-addons-test-utils": "15.4.2",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-mock-store": "1.2.2",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.16.1"
  }
}
